{
  "name": "paimon-gold-subgraph",
  "version": "0.1.0",
  "description": "The Graph subgraph for Paimon Gold Protocol - indexes positions, trades, liquidity events",
  "license": "MIT",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ paimon-gold-protocol",
    "deploy:hosted": "graph deploy --product hosted-service YOUR_GITHUB/paimon-gold-protocol",
    "create-local": "graph create --node http://localhost:8020/ paimon-gold-protocol",
    "remove-local": "graph remove --node http://localhost:8020/ paimon-gold-protocol",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 paimon-gold-protocol",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.71.0",
    "@graphprotocol/graph-ts": "^0.35.1"
  },
  "devDependencies": {
    "matchstick-as": "^0.6.0"
  }
}
